RewriteEngine On

# Rewrite rule for product pages
RewriteRule ^job/([a-zA-Z0-9_-]+)/?$ job.php?page_url=$1 [L,QSA]

# Rewrite rule for blog pages
RewriteRule ^blog/([a-zA-Z0-9_-]+)/?$ blog.php?page_url=$1 [L,QSA]

